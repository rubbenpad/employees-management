{
  "version": 2,
  "builds": [
    {
      "use": "@now/static-build",
      "src": "package.json"
    }
  ],
  "routes": [
    {
      "src": "(.*).js",
      "dest": "/$1.js"
    },
    {
      "src": "(.*).png",
      "dest": "/$1.png"
    },
    {
      "src": "/.*",
      "dest": "index.html"
    }
  ],
  "env": {
    "URI": "@uri_api",
  }
}
